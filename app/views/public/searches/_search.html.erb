  <div class="search_form">
    <%= form_with url: search_path, method: :get do |f| %>
      <%= f.text_field :word %>
      <%= f.select :range, options_for_select([[I18n.t('search.user')], [I18n.t('search.post')]]) %>
      <%= f.submit I18n.t('search.search'), class: "btn btn-primary"  %>
    <% end %>
  </div>
